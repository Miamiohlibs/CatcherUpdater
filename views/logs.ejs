<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <%- include('partials/bootstrap') %>
</head>
<body>
    <%- include('partials/nav') %>

    <% let batchList = '' %>
    <% let time = '' %>
    <% batches.forEach(b => { %>
        <% var time = new Date((b.created)); %>
        <% batchList = `<tr><td><a href="/logs/${b.batchId}">${b.batchId}</a></td>` +
        `<td>${b.batchName}</td>` +
        `<td>${b.successes}</td>` +
        `<td>${b.failures}</td>` +
        `<td>${b.collectionAlias}</td>` +
        `<td>${time}</td></tr>` +
            `</tr>`   %>   
    <% }) %>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">BatchId</th>
                <th scope="col">BatchName</th>
                <th scope="col">Successes</th>
                <th scope="col">Failures</th>
                <th scope="col">Collection</th>
                <th scope="col">Time</th>

            </tr>
        </thead>
        <tbody>
            <%- batchList %>
        </tbody>
    </table>
    <hr />
    <% let list = '' %>
    <% transactions.forEach(t => { %>
        <% var time = new Date(parseInt(t)); %>
        <% list = `<tr><td><a href="/logs/${t}">${t}</a></td><td>${time}</td></tr>` +list  %>
    <% }) %>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Transaction</th>
                <th scope="col">Time</th>
            </tr>
        </thead>
        <tbody>
            <%- list %>
        </tbody>
    </table>
</body>
</html>